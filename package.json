{
  "name": "use-system-theme",
  "version": "0.0.4",
  "description": "React Hook for System Theme",
  "author": "xcv58",
  "license": "MIT",
  "repository": "xcv58/use-system-theme",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "watch:yalc": "nodemon -x yalc push",
    "release": "standard-version",
    "prepare": "yarn run build",
    "predeploy": "cd examples/create-react-app && yarn install && yarn run build",
    "deploy": "gh-pages -d examples/create-react-app/build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@svgr/rollup": "4.3.3",
    "@types/jest": "24.0.23",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.4",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "cross-env": "6.0.3",
    "gh-pages": "2.1.1",
    "husky": "3.1.0",
    "nodemon": "2.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts-ts": "3.1.0",
    "rollup": "1.27.7",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-typescript2": "0.25.2",
    "rollup-plugin-url": "3.0.1",
    "standard-version": "7.0.1",
    "typescript": "3.7.2",
    "yalc": "1.0.0-pre.34"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "react",
    "react-hooks",
    "theme",
    "typescript",
    "ssr"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
