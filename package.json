{
  "name": "use-system-theme",
  "version": "0.1.1",
  "description": "React Hook for System Theme",
  "author": "xcv58",
  "license": "MIT",
  "repository": "xcv58/use-system-theme",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "watch:yalc": "nodemon -x yalc push",
    "release": "standard-version",
    "prepare": "yarn run build",
    "predeploy": "cd examples/create-react-app && yarn install && yarn run build",
    "deploy": "gh-pages -d examples/create-react-app/build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@svgr/rollup": "5.5.0",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "cross-env": "7.0.3",
    "gh-pages": "3.1.0",
    "husky": "4.3.8",
    "nodemon": "2.0.7",
    "prettier": "2.2.0",
    "pretty-quick": "3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts-ts": "3.1.0",
    "rollup": "2.33.3",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-typescript2": "0.29.0",
    "rollup-plugin-url": "3.0.1",
    "standard-version": "9.0.0",
    "typescript": "4.1.2",
    "yalc": "1.0.0-pre.47"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "react",
    "react-hooks",
    "theme",
    "typescript",
    "ssr"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
